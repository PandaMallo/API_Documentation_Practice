<!--Simple xml-->

<?xml version="1.0" ?>
<dailyForecast>
    <date>2015-06-01</date>
    <description>sunny</description>
    <maxTemp unit="C">22</maxTemp>
    <minTemp unit="C">20</minTemp>
    <windSpeed unit="kph">12</windSpeed>
    <danger>false</denger>
</dailyForecast>


<!--Complex xml-->

<?xml version="1.0" ?>
<forecast>
